# Strategies

# compile and produce all intermediate files
# node compiler.js -i ./chinbreak.js -a @grumdrig.asm -d @grumdrig.dasm -b @grumdrig.bin -s -p @grumdrig.strat grumdrig.slog

--for STRAT in grumdrig.slog quester.slog do node chinbreak.js -v -c $STRAT

# tests

node compiler.js -a @test.asm test.slog
node --trace-uncaught vm.js -a test.asm -d @test.dasm -o @test.bin -- test.asm
node test.js -- test.bin

# Manuals

node_modules/marked/bin/marked.js -i vm.md -o @vm.html
node_modules/marked/bin/marked.js -i assembler_manual.md -o @asm.html
node_modules/marked/bin/marked.js -i language.md -o @language.html
node chinbreak.js --generate-documentation > @chinbreak-doc.html

# Snake

node compiler.js -i ./snake.js -a @steve.asm steve.slog
node vm.js -a steve.asm -o @steve.bin -- steve.asm
node snake.js steve.bin -- steve.bin

